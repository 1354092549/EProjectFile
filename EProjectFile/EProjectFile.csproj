<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net40</TargetFrameworks>
    <Title>Epl Project Editor</Title>
    <Description>Epl Project Editor</Description>
    <Company>QIQI</Company>
    <Copyright>BTC Address:1NaMvWjj4dFnK7av7rBSUu3Bsm2ESrU7Cf</Copyright>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>QIQI.EProjectFile</PackageId>
    <PackageVersion>1.1.0</PackageVersion>
    <Tags>EPL</Tags>
    <Authors>QIQI</Authors>
    <PackageProjectUrl>https://github.com/1354092549/EProjectFile</PackageProjectUrl>
    <RootNamespace>QIQI.EProjectFile</RootNamespace>

  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
    <PackageReference Include="StyleCop.MSBuild" Version="6.0.0">
      <PrivateAssets>all</PrivateAssets>
      <ExcludeAssets>Runtime</ExcludeAssets>
      <IncludeAssets>compile; build; native; contentfiles; analyzers</IncludeAssets>
     </PackageReference>

    <Content Include="bin/Release/LibNameInfoToJson.exe;QIQI.EProjectFile.targets">
      <Pack>true</Pack>
      <PackagePath>build</PackagePath>
    </Content>
  </ItemGroup>
</Project>
